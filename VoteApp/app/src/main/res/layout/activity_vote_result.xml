            android:text="명화이름 1"
            android:textSize="15dp"/>
        <RatingBar
            android:id="@+id/rtBar1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="?android:attr/ratingBarStyleSmall"
            android:numStars="5"/>
    </LinearLayout>
